{
    "manifest_version": 3,
    "name": "Youtube Video Classifier",
    "version": "1.0",
    "permissions": ["activeTab", "declarativeContent", "storage"],
    "host_permissions": ["<all_urls>"],
    "action": {
        "default_popup": "./home/home.html",
        "default_icon": {
            "16": "images/icon.png",
            "32": "images/icon.png",
            "48": "images/icon.png",
            "128": "images/icon.png"
        }
    },
    "background": {
        "service_worker": "background.js"
    }
}
